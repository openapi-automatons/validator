{
  "name": "@automatons/validator",
  "version": "1.0.186",
  "main": "index.js",
  "author": "tanmen <yt.prog@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "depcheck && node scripts/prebuild.js",
    "build": "tsc && cp package.json README.md LICENSE dist",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "postinstall": "husky install && typesync && yarn check --integrity || yarn install --ignore-scripts && yarn-deduplicate",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "postversion": "cp package.json ../package.json"
  },
  "keywords": [
    "openapi",
    "validate",
    "validator"
  ],
  "dependencies": {
    "@hyperjump/json-schema": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/npm": "^10.0.5",
    "@types/eslint": "^8.44.2",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^27.5.0",
    "@types/semantic-release": "^20.0.1",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "cp": "^0.2.0",
    "depcheck": "^1.4.5",
    "eslint": "^8.49.0",
    "eslint-config-google": "^0.14.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "lint-staged": "^14.0.1",
    "pinst": "^3.0.0",
    "semantic-release": "^21.1.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.8.4",
    "typesync": "^0.11.1",
    "yarn-deduplicate": "^6.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
