    "@types/eslint": "^7.28.2",
    "@types/jest": "^27.0.2",
    "@types/semantic-release": "^17.2.3",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "cp": "^0.2.0",
    "depcheck": "^1.4.2",
    "eslint": "^8.2.0",
