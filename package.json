{
  "name": "@automatons/validator",
  "version": "1.0.15",
  "main": "index.js",
  "author": "tanmen <yt.prog@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "depcheck && node scripts/prebuild.js",
    "build": "tsc && cp package.json README.md LICENSE dist",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "postinstall": "husky install && typesync && yarn check --integrity || yarn install --ignore-scripts && yarn-deduplicate",
    "postversion": "cp package.json ../package.json"
  },
  "keywords": [
    "openapi",
    "validate",
    "validator"
  ],
  "dependencies": {
    "@hyperjump/json-schema": "^0.18.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/npm": "^8.0.0",
    "@types/eslint": "^8.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.3",
    "@types/semantic-release": "^17.2.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "cp": "^0.2.0",
    "depcheck": "^1.4.2",
    "eslint": "^8.4.1",
    "eslint-config-google": "^0.14.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "semantic-release": "^18.0.1",
    "ts-jest": "^27.1.0",
    "typescript": "^4.5.2",
    "typesync": "^0.8.0",
